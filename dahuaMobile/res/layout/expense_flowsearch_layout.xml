<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/linegray_bottom"
    android:descendantFocusability="blocksDescendants"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/expense_flowsearch_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="10dip"
        android:paddingLeft="0dip"
        android:paddingRight="5dip"
        android:paddingTop="10dip" 
        android:stretchColumns="0">

        <TextView
            android:id="@+id/expense_flowsearch_layout_FId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="" />

        <TextView
            android:id="@+id/expense_flowsearch_layout_FItemName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/expense_flowsearch_layout_FId"
            android:paddingLeft="5dip"
            android:text="" />
    </RelativeLayout>

</RelativeLayout>